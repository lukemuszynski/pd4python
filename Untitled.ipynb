   "source": [
    "file_object  = open(\"HTML Color Values.html\", \"r\")\n",
    "colors = []\n",
    "for line in file_object:\n",
    "    if '    <td style=\"background-color:' in line: \n",
    "        start_index = line.find(\":\")\n",
    "        end_index = start_index+8\n",
    "        colors.append(line[start_index+1:end_index])"
   ]
